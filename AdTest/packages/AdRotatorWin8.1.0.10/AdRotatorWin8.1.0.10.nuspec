    <id>AdRotatorWin8</id>
    <version>1.0.10</version>
    <title>Ad Rotator for Windows 8</title>
    <authors>Simon Jackson, Gergely Orosz</authors>
    <owners>Simon Jackson, Gergely Orosz</owners>
    <projectUrl>http://wp7adrotator.codeplex.com/</projectUrl>
